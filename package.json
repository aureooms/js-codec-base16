{
  "name": "@aureooms/js-codec-base16",
  "description": "Base16 encoder/decoder for JavaScript",
  "version": "1.0.0",
  "author": "aureooms",
  "ava": {
    "require": [
      "babel-polyfill",
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-codec-base16/issues"
  },
  "dependencies": {
    "@aureooms/js-codec": "^2.0.0",
    "@aureooms/js-itertools": "^3.3.0"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "babel-cli": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-latest": "6.24.1",
    "codeclimate-test-reporter": "0.5.1",
    "coveralls": "2.13.3",
    "esdoc": "0.5.2",
    "nyc": "14.1.1"
  },
  "homepage": "https://aureooms.github.io/js-codec-base16",
  "keywords": [
    "base16",
    "codec",
    "decode",
    "encode",
    "strings"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-codec-base16"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "prepublish": "npm run build",
    "test": "ava ./test/src"
  }
}
